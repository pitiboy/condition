{
  "name": "react-conditional-render",
  "version": "0.0.1",
  "description": "React component to handle if-else like conditions in JSX",
  "keywords": [
    "React",
    "if-else",
    "condition"
  ],
  "main": "./app/index.js",
  "scripts": {
    "dev": "NODE_ENV=development AUTO_LOGIN=1 webpack-dev-server --content-base build/ --config webpack.config.js --hot --inline",
    "test": "jest",
    "cleanbuild": "rm -rf ./build/*.js;rm -rf ./build/react-conditional-render*.css",
    "prebuild": "yarn test",
    "build": "yarn cleanbuild; NODE_ENV=production webpack -p --config webpack.config.js"
  },
  "author": "Peter Horvath <horvath.peter@fejlesztesek.hu>",
  "contributors": "N/A",
  "repository": "https://github.com/pitiboy/react-conditional-render",
  "bugs": "https://github.com/pitiboy/react-conditional-render/issues",
  "license": "Apache 2.0",
  "homepage": "https://react-conditional-render.surge.sh",
  "homepage_test": "https://localhost:3000",
  "homepage_dev": {
    "host": "0.0.0.0",
    "port": 3000
  },
  "supportedLanguages": {
    "en": "English"
  },
  "supportedPlatforms": {
    "prod": "Production",
    "test": "Test"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "breakpoint-sass": "^2.7.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "jest-enzyme": "^2.1.0",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "sass-bem": "^2.6.5",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.1.0-beta.12"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "classnames": "^2.2.5",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "2.22.0",
    "prop-types": "^0.2.0",
    "rc-queue-anim": "^0.13.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-style-proptype": "^3.0.0"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "^.+\\.(s?css|less|svg)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "./app/SomeTest.spec.js"
    ]
  }
}
